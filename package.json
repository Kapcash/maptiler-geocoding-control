{
  "name": "@maptiler/geocoding-control",
  "version": "0.0.59",
  "type": "module",
  "author": {
    "name": "Martin Å½dila",
    "email": "martin.zdila@maptiler.com"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/maptiler/maptiler-geocoding-control"
  },
  "bugs": {
    "url": "https://github.com/maptiler/maptiler-geocoding-control/issues"
  },
  "keywords": [
    "geocoding",
    "forward-geocoding",
    "reverse-geocoding",
    "maptiler",
    "leaflet",
    "maplibre",
    "map",
    "search",
    "leaflet-control",
    "maplibre-control",
    "control"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && mv dist/maplibregl.umd.cjs dist/maplibregl.umd.js && FLAVOUR=leaflet vite build && mv dist/leaflet.umd.cjs dist/leaflet.umd.js && tsc --outDir dist --declaration --emitDeclarationOnly",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "files": [
    "dist",
    "src/lib"
  ],
  "exports": {
    "./leaflet": {
      "import": "./dist/leaflet.js",
      "require": "./dist/leaflet.umd.js",
      "types": "./dist/lib/LeafletGeocodingControl.d.ts"
    },
    "./maplibregl": {
      "import": "./dist/maplibregl.js",
      "require": "./dist/maplibregl.umd.js",
      "types": "./dist/lib/MaplibreglGeocodingControl.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./src/lib/GeocodingControl.svelte": {
      "import": "./src/lib/GeocodingControl.svelte",
      "require": "./src/lib/GeocodingControl.svelte"
    }
  },
  "types": "./dist/lib/index.d.ts",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "@turf/buffer": "^6.5.0",
    "@turf/difference": "^6.5.0",
    "@turf/union": "^6.5.0",
    "@types/leaflet": "^1.9.0",
    "prettier": "^2.8.3",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-preprocess": "^5.0.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.0.4"
  },
  "peerDependencies": {
    "leaflet": "^1.9.3",
    "maplibre-gl": "^2.4.0"
  },
  "peerDependenciesMeta": {
    "leaflet": {
      "optional": true
    },
    "maplibre-gl": {
      "optional": true
    }
  }
}
